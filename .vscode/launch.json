{
    "configurations": [
        {
            "name": "Launch Chrome",
            "request": "launch",
            "type": "chrome",
            "url": "http://localhost:3000",
            "webRoot": "${workspaceFolder}",
            "sourceMaps": true,
            "sourceMapPathOverrides": {
                "webpack://_N_E/*": "${workspaceFolder}/*",
                "webpack:///./*": "${workspaceFolder}/*",
                "webpack:///*": "${workspaceFolder}/*"
            }
        }
    ]
}